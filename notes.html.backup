        // 切换展开/收起状态
        function toggleSection(sectionId) {
            const section = document.getElementById(`${sectionId}-section`);
            const icon = document.getElementById(`${sectionId}-toggle-icon`);
            
            section.classList.toggle('expanded');
            icon.classList.toggle('rotate-180');
        }

        // 更新表单字段（根据笔记类型）
        function updateFormFields() {
            // 可以根据需要添加特定类型的字段
        }

        // 添加标签
        function addTag() {
            const tagInput = document.getElementById('tag-input');
            const tagValue = tagInput.value.trim();
            
            if (tagValue && !currentTags.includes(tagValue)) {
                currentTags.push(tagValue);
                renderTags();
                tagInput.value = '';
            }
        }

        // 移除标签
        function removeTag(tagToRemove) {
            currentTags = currentTags.filter(tag => tag !== tagToRemove);
            renderTags();
        }

        // 渲染标签
        function renderTags() {
            const tagsContainer = document.getElementById('tags-container');
            tagsContainer.innerHTML = currentTags.map(tag => `
                <span class="inline-flex items-center px-3 py-1 rounded-full text-xs font-medium bg-indigo-100 text-indigo-800">
                    ${tag}
                    <button
                        type="button"
                        class="flex-shrink-0 ml-2 h-4 w-4 rounded-full inline-flex items-center justify-center text-indigo-500 hover:bg-indigo-200 hover:text-indigo-600 focus:outline-none focus:bg-indigo-500 focus:text-white"
                        onclick="removeTag('${tag}')"
                    >
                        <span class="text-xs">×</span>
                    </button>
                </span>
            `).join('');
        }

        // 从表单添加笔记
        function addNoteFromForm() {
            const type = document.getElementById('note-type').value;
            const content = document.getElementById('note-content').value.trim();
            const translation = document.getElementById('note-translation').value.trim();
            
            if (!content) {
                alert('请输入笔记内容');
                return;
            }
            
            const note = {
                id: Date.now(),
                type: type,
                content: content,
                translation: translation,
                tags: [...currentTags],
                createdAt: new Date().toLocaleString('zh-CN')
            };
            
            notes.unshift(note);
            saveNotes();
            loadAllContent();
            
            // 清空表单
            document.getElementById('note-content').value = '';
            document.getElementById('note-translation').value = '';
            currentTags = [];
            renderTags();
        }

        // 删除笔记
        function deleteNote(id) {
            if (confirm('确定要删除这条笔记吗？')) {
                notes = notes.filter(note => note.id !== id);
                saveNotes();
                loadAllContent();
            }
        }

        // 保存笔记到本地存储
        function saveNotes() {
            localStorage.setItem('cet6-notes', JSON.stringify(notes));
        }

        // 搜索处理
        function handleSearch() {
            const searchTerm = document.getElementById('search-input').value.toLowerCase();
            loadNotesByType(searchTerm);
        }

        // 显示例句模态框
        function showPhraseExample(phrase) {
            const modal = document.getElementById('example-modal');
            const title = modal.querySelector('.modal-title');
            const exampleText = modal.querySelector('.modal-example-text');
            
            title.textContent = phrase;
            
            // 显示中文翻译和例句
            const note = notes.find(n => n.content === phrase && n.type === 'phrase');
            if (note && phraseExamples[phrase]) {
                exampleText.innerHTML = `
                    <div class="mb-4">
                        <div class="text-gray-600 text-lg mb-2">中文含义：</div>
                        <div class="text-gray-800 font-medium">${note.translation}</div>
                    </div>
                    <div>
                        <div class="text-gray-600 text-lg mb-2">英文例句：</div>
                        <div class="text-gray-800">${phraseExamples[phrase]}</div>
                    </div>
                `;
            } else {
                exampleText.textContent = '暂无例句';
            }
            
            modal.classList.remove('hidden');
        }

        // 关闭例句模态框
        function closeExampleModal() {
            const modal = document.getElementById('example-modal');
            modal.classList.add('hidden');
        }

        // 加载形近词并按组显示
        function loadConfusableWords() {
            const container = document.getElementById('confusable-container');
            const countElement = document.getElementById('confusable-count');
            
            // 获取所有形近词
            const confusableNotes = notes.filter(note => note.type === 'confusable');
            
            // 更新计数
            const groups = {};
            confusableNotes.forEach(note => {
                if (!groups[note.group]) {
                    groups[note.group] = [];
                }
                groups[note.group].push(note);
            });
            
            countElement.textContent = `共 ${Object.keys(groups).length} 组`;
            
            // 渲染形近词组
            let html = '';
            
            for (const groupId in groups) {
                const groupNotes = groups[groupId];
                html += `
                    <div class="confusable-group">
                        <div class="confusable-group-header" onclick="toggleConfusableGroup('${groupId}')">
                            <span class="font-bold text-gray-800 flex items-center">
                                <svg class="w-5 h-5 mr-2 text-gray-600" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12h6m-6 4h6m2 5H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z"></path>
                                </svg>
                                ${groupId} (${groupNotes.length} 个)
                            </span>
                            <svg class="h-5 w-5 text-gray-600 transform transition-transform" 
                                 id="confusable-arrow-${groupId}"
                                 xmlns="http://www.w3.org/2000/svg" 
                                 viewBox="0 0 20 20" 
                                 fill="currentColor">
                                <path fill-rule="evenodd" d="M5.293 7.293a1 1 0 011.414 0L10 10.586l3.293-3.293a1 1 0 111.414 1.414l-4 4a1 1 0 01-1.414 0l-4-4a1 1 0 010-1.414z" clip-rule="evenodd" />
                            </svg>
                        </div>
                        <div class="confusable-group-content" id="confusable-content-${groupId}">
                            <div class="grid grid-cols-1 sm:grid-cols-2 md:grid-cols-3 lg:grid-cols-4 gap-4">
                                ${groupNotes.map(note => `
                                    <div class="confusable-word-card">
                                        <button 
                                            onclick="deleteNote(${note.id})"
                                            class="delete-button text-gray-400 hover:text-red-500 transition-colors duration-200"
                                            aria-label="删除笔记"
                                        >
                                            <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5" viewBox="0 0 20 20" fill="currentColor">
                                                <path fill-rule="evenodd" d="M9 2a1 1 0 00-.894.553L7.382 4H4a1 1 0 000 2v10a2 2 0 002 2h8a2 2 0 002-2V6a1 1 0 100-2h-3.382l-.724-1.447A1 1 0 0011 2H9zM7 8a1 1 0 012 0v6a1 1 0 11-2 0V8zm5-1a1 1 0 00-1 1v6a1 1 0 102 0V8a1 1 0 00-1-1z" clip-rule="evenodd" />
                                            </svg>
                                        </button>
                                        
                                        <div class="confusable-word-content">${note.content}</div>
                                        <div class="confusable-word-translation">${note.translation}</div>
                                        
                                        <div class="confusable-word-tags">
                                            ${note.tags.map(tag => `
                                                <span class="confusable-word-tag">
                                                    ${tag}
                                                </span>
                                            `).join('')}
                                        </div>
                                    </div>
                                `).join('')}
                            </div>
                        </div>
                    </div>
                `;
            }
            
            container.innerHTML = html;
            
            // 默认展开所有组
            for (const groupId in groups) {
                const content = document.getElementById(`confusable-content-${groupId}`);
                if (content) {
                    content.classList.remove('hidden');
                }
                const arrow = document.getElementById(`confusable-arrow-${groupId}`);
                if (arrow) {
                    arrow.classList.add('rotate-180');
                }
            }
        }

        // 切换形近词组展开/收起
        function toggleConfusableGroup(groupId) {
            const content = document.getElementById(`confusable-content-${groupId}`);
            const arrow = document.getElementById(`confusable-arrow-${groupId}`);
            
            content.classList.toggle('hidden');
            arrow.classList.toggle('rotate-180');
        }

        // 按类型加载和显示笔记
        function loadNotesByType(searchTerm = '') {
            const container = document.getElementById('notes-container');
            
            // 过滤笔记
            let filteredNotes = notes;
            if (searchTerm) {
                filteredNotes = notes.filter(note => 
                    note.content.toLowerCase().includes(searchTerm) || 
                    (note.translation && note.translation.toLowerCase().includes(searchTerm)) ||
                    note.tags.some(tag => tag.toLowerCase().includes(searchTerm))
                );
            }
            
            // 按类型分组（排除形近词，因为已在专门区域显示）
            const notesByType = {
                'word': { label: '单词', notes: [], colorClass: 'bg-blue-50 border-l-4 border-blue-500' },
                'phrase': { label: '词组', notes: [], colorClass: 'bg-green-50 border-l-4 border-green-500' },
                'sentence': { label: '句子', notes: [], colorClass: 'bg-purple-50 border-l-4 border-purple-500' },
                'summary': { label: '总结', notes: [], colorClass: 'bg-indigo-50 border-l-4 border-indigo-500' }
            };
            
            filteredNotes.forEach(note => {
                if (note.type !== 'confusable' && notesByType[note.type]) {
                    notesByType[note.type].notes.push(note);
                }
            });
            
            // 渲染每个类型的笔记
            let html = '';
            
            for (const type in notesByType) {
                const typeData = notesByType[type];
                if (typeData.notes.length > 0) {
                    const colorClasses = {
                        'word': 'bg-blue-100 text-blue-800',
                        'phrase': 'bg-green-100 text-green-800',
                        'sentence': 'bg-purple-100 text-purple-800',
                        'summary': 'bg-indigo-100 text-indigo-800'
                    };
                    
                    const typeIcons = {
                        'word': 'M9 12h6m-6 4h6m2 5H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z',
                        'phrase': 'M7 8h10M7 12h4m1 8l-4-4H5a2 2 0 01-2-2V6a2 2 0 012-2h14a2 2 0 012 2v8a2 2 0 01-2 2h-3l-4 4z',
                        'sentence': 'M13 10h5M4 19V5a2 2 0 012-2h6a2 2 0 012 2v6',
                        'summary': 'M9 5H7a2 2 0 00-2 2v10a2 2 0 002 2h8a2 2 0 002-2V7a2 2 0 00-2-2h-2M9 5a2 2 0 002 2h2a2 2 0 002-2M9 5a2 2 0 012-2h2a2 2 0 012 2m-3 7h3m-3 4h3m-6-4h.01M9 16h.01'
                    };
                    
                    html += `
                        <div class="bg-white rounded-2xl shadow-lg note-card">
                            <div 
                                class="px-6 py-4 cursor-pointer flex justify-between items-center ${typeData.colorClass}"
                                onclick="toggleSection('${type}')"
                            >
                                <h3 class="text-lg font-semibold text-gray-800 flex items-center">
                                    <span class="inline-flex items-center px-3 py-1 rounded-full text-sm font-medium mr-3 ${colorClasses[type]}">
                                        ${typeData.label}
                                    </span>
                                    <span class="text-gray-600 text-base">共 ${typeData.notes.length} 条笔记</span>
                                </h3>
                                <svg 
                                    class="h-5 w-5 transform transition-transform text-gray-600"
                                    xmlns="http://www.w3.org/2000/svg" 
                                    viewBox="0 0 20 20" 
                                    fill="currentColor"
                                    id="${type}-toggle-icon"
                                >
                                    <path fill-rule="evenodd" d="M5.293 7.293a1 1 0 011.414 0L10 10.586l3.293-3.293a1 1 0 111.414 1.414l-4 4a1 1 0 01-1.414 0l-4-4a1 1 0 010-1.414z" clip-rule="evenodd" />
                                </svg>
                            </div>
                            
                            <div id="${type}-section" class="expandable-section expanded">
                                <div class="p-6 bg-gradient-to-b from-white to-gray-50">
                                    <div class="grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-3 xl:grid-cols-4 gap-6">
                                        ${typeData.notes.map(note => `
                                            <div 
                                                class="note-card-item"
                                                ${note.type === 'phrase' ? `onclick="showPhraseExample('${note.content}')"` : ''}
                                                style="cursor: ${note.type === 'phrase' ? 'pointer' : 'default'}"
                                            >
                                                <button 
                                                    onclick="event.stopPropagation(); deleteNote(${note.id})"
                                                    class="delete-button text-gray-400 hover:text-red-500 transition-colors duration-200"
                                                    aria-label="删除笔记"
                                                >
                                                    <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5" viewBox="0 0 20 20" fill="currentColor">
                                                        <path fill-rule="evenodd" d="M9 2a1 1 0 00-.894.553L7.382 4H4a1 1 0 000 2v10a2 2 0 002 2h8a2 2 0 002-2V6a1 1 0 100-2h-3.382l-.724-1.447A1 1 0 0011 2H9zM7 8a1 1 0 012 0v6a1 1 0 11-2 0V8zm5-1a1 1 0 00-1 1v6a1 1 0 102 0V8a1 1 0 00-1-1z" clip-rule="evenodd" />
                                                    </svg>
                                                </button>
                                                
                                                <div class="note-card-content">${note.content}</div>
                                                ${note.translation ? `<div class="note-card-translation">${note.translation}</div>` : ''}
                                                
                                                ${note.tags.length > 0 ? `
                                                    <div class="note-card-tags">
                                                        ${note.tags.map(tag => {
                                                            let tagClass = 'note-card-tag';
                                                            if (note.type === 'word') tagClass += ' word-note-tag';
                                                            if (note.type === 'phrase') tagClass += ' phrase-note-tag';
                                                            if (note.type === 'sentence') tagClass += ' sentence-note-tag';
                                                            if (note.type === 'summary') tagClass += ' summary-note-tag';
                                                            
                                                            return `
                                                                <span class="${tagClass}">
                                                                    ${tag}
                                                                </span>
                                                            `;
                                                        }).join('')}
                                                    </div>
                                                ` : ''}
                                            </div>
                                        `).join('')}
                                    </div>
                                </div>
                            </div>
                        </div>
                    `;
                }
            }
            
            container.innerHTML = html;
        }

        // 加载所有内容
        function loadAllContent() {
            loadConfusableWords();
            loadNotesByType();
        }

        // 页面加载完成后初始化
        document.addEventListener('DOMContentLoaded', function() {
            loadAllContent();
            
            // 点击模态框外部关闭
            document.getElementById('example-modal').addEventListener('click', function(e) {
                if (e.target === this) {
                    closeExampleModal();
                }
            });
        });
    </script>
